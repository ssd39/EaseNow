{
  "exe": "core",
  "key": "private.pem",
  "debug": true,
  "heapSize": 512,
  "executableHeap": false,
  "productID": 1,
  "securityVersion": 1,
  "mounts": [
    {
      "source": "./data",
      "target": "/data",
      "type": "hostfs",
      "readOnly": false
    }
  ],
  "env": null,
  "files": [
    {
      "source": "./certs/ipfs-server.pem",
      "target": "/etc/ssl/certs/ipfs-server.crt"
    },
    {
      "source": "./certs/base-org.pem",
      "target": "/etc/ssl/certs/base-org.crt"
    }
  ]
}
